<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.AdministrationPane">
    <top>
      <HBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#back" prefHeight="64.0" prefWidth="100.0" text="%return">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
              <VBox prefHeight="90.0" prefWidth="530.0">
                  <fx:include fx:id="topMenu" source="bar.fxml" />
               <padding>
                  <Insets bottom="10.0" />
               </padding>
              </VBox>
         </children>
      </HBox>
    </top>
   <center>
      <TableView prefHeight="297.0" prefWidth="600.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="254.39996033906937" text="%name" />
          <TableColumn prefWidth="193.6000213623047" text="%surname" />
            <TableColumn prefWidth="832.0" text="%post" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <HBox prefHeight="18.0" prefWidth="400.0" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea focusTraversable="false" prefHeight="38.0" prefWidth="947.0" />
            <Button mnemonicParsing="false" prefHeight="39.0" prefWidth="103.0" text="%profile" />
         </children>
         <BorderPane.margin>
            <Insets left="100.0" right="100.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
