<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.AdministrationPane">
    <top>
        <VBox prefHeight="0.0" prefWidth="551.0" BorderPane.alignment="CENTER">
            <fx:include fx:id="topMenu" source="bar.fxml" />
        </VBox>
    </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane alignment="BOTTOM_RIGHT" cache="true" hgap="64.0" layoutX="10.0" snapToPixel="false" vgap="100.0">
              <columnConstraints>
                <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" prefWidth="100.0" />
                <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="64.0" prefHeight="64.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="64.0" prefHeight="64.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="hire_btn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#hire_btn" prefHeight="64.0" prefWidth="600.0" text="%hire" />
                  <Button fx:id="staff_btn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#staff_btn" prefHeight="64.0" prefWidth="600.0" text="%personel" GridPane.rowIndex="1" />
                  <Button fx:id="reg_emp_btn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="64.0" prefWidth="600.0" text="%registerPatient" GridPane.columnIndex="1" />
                  <Button fx:id="inventory_btn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="64.0" prefWidth="600.0" text="%inventory" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <opaqueInsets>
                  <Insets top="1000.0" />
               </opaqueInsets>
               <padding>
                  <Insets top="50.0" />
               </padding>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </GridPane>
         </children>
      </VBox>
   </center>
</BorderPane>
