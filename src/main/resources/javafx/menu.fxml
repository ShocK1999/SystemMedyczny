<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.MenuController">
   <children>
      <ToggleButton mnemonicParsing="false" onAction="#open_profil" prefHeight="60.0" prefWidth="80.0" text="%profile">
         <toggleGroup>
            <ToggleGroup fx:id="toggleMenu" />
         </toggleGroup></ToggleButton>
      <ToggleButton mnemonicParsing="false" onAction="#change_language" prefHeight="60.0" text="%languChange" toggleGroup="$toggleMenu">
         <graphic>
            <ImageView fitHeight="48.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/img_language.png" />
               </image>
            </ImageView>
         </graphic></ToggleButton>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
