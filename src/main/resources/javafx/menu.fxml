<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="83.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.MenuController">
   <children>
      <ToggleButton mnemonicParsing="false" onAction="#open_profil" prefHeight="60.0" prefWidth="80.0">
         <toggleGroup>
            <ToggleGroup fx:id="toggleMenu" />
         </toggleGroup>
         <graphic>
            <ImageView fitHeight="54.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/img_profile.png" />
               </image>
            </ImageView>
         </graphic></ToggleButton>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ChoiceBox fx:id="languChange" prefHeight="22.0" prefWidth="135.0" />
            <Button fx:id="languBtn" mnemonicParsing="false" onAction="#change_language" prefHeight="26.0" prefWidth="133.0" text="%languChange" GridPane.rowIndex="1" />
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </GridPane>
      <Button fx:id="btnLogOut" mnemonicParsing="false" onAction="#log_out" prefHeight="62.0" prefWidth="69.0" text="%logOut" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
